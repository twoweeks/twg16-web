        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "antd": "^4.1.4",
        "ky": "^0.19.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2",
        "reaptcha": "^1.7.2"
    },
    "devDependencies": {
        "@prettier/plugin-php": "^0.14.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.3",
        "husky": "^4.2.5",
        "lint-staged": "^10.1.6",
        "prettier": "^2.0.4",
        "react-scripts": "3.4.1",
        "sass": "^1.26.3"
    }
}
