{
    "name": "twg16-web",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "eslint": "eslint src/",
        "prettier": "prettier --write **/*.{html,js,jsx,json,scss,php}",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "*.{html,js,jsx,json,scss,php}": [
            "prettier --write"
        ]
    },
